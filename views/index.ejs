  <script>
    var startingTickAngle = -135; //get rectangular geometric data of knob (x, y, width, height)

    const collection = document.getElementsByClassName("knob-surround");
    for (let i = 0; i < collection.length; i++) {
      let tickcontainer = collection[i].getElementsByClassName("ticks")[0].id;

      angle = parseInt(collection[i].getElementsByClassName("knob")[0].id);

      var volumeKnob = document.getElementById(angle);
      var boundingRectangle = volumeKnob.getBoundingClientRect();

      finalAngleInDegrees = (angle / 100) * 270;

      createTicks(28, 0, tickcontainer);
      volumeKnob.style.transform =
        "rotate(" + (parseInt(finalAngleInDegrees) - 5) + "deg)"; //use dynamic CSS transform to rotate volume knob
      tickHighlightPosition = Math.round((angle * 2.7) / 10);

      createTicks(28, tickHighlightPosition, tickcontainer); //highlight
    }

    function main() {}

    function createTicks(numTicks, highlightNumTicks, tickContainerId) {
      tickContainer = document.getElementById(tickContainerId);
      //reset first by deleting all existing ticks
      while (tickContainer.firstChild) {
        tickContainer.removeChild(tickContainer.firstChild);
      }

      //create ticks
      for (var i = 0; i < numTicks; i++) {
        var tick = document.createElement("div");

        //highlight only the appropriate ticks using dynamic CSS
        if (i < highlightNumTicks) {
          tick.className = "tick activetick";
        } else {
          tick.className = "tick";
        }

        tickContainer.appendChild(tick);
        tick.style.transform = "rotate(" + startingTickAngle + "deg)";
        startingTickAngle += 10;
      }

      startingTickAngle = -135; //reset
    }
    main();
  </script>
